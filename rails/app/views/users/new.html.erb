<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<h1>Sign upページです</h2>
<%= form_for(@user) do |f|%>

    <%= f.label :name %>
    <%= f.text_field :name %> 
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <br>
    <%= f.label :password%>
    <%= f.password_field :password_digest%>

    <br>
    <%= f.submit "Create my account" %>
<% end %>